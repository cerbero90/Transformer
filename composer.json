{
    "name": "cerbero/transformer",
    "type": "library",
    "description": "Transform, manipulate, cast and map objects and arrays.",
    "keywords": [
        "transformer",
        "transform",
        "manipulate",
        "map",
        "cast",
        "object",
        "array"
    ],
    "homepage": "https://github.com/cerbero90/Transformer",
    "license": "MIT",
    "authors": [
        {
            "name": "Andrea Marco Sartori",
            "email": "andrea.marco.sartori@gmail.com",
            "homepage": "https://github.com/cerbero90",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.0",
        "illuminate/support": "^5.5|^6.0|^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=6.5",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Cerbero\\Transformer\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cerbero\\Transformer\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}